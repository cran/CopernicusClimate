Package: CopernicusClimate
Title: Search Download and Handle Data from Copernicus Climate Data
        Service
Version: 0.0.3
Authors@R: 
    c(
      person("Pepijn", "de Vries", role = c("aut", "cre"),
             email = "pepijn.devries@outlook.com",
             comment = c(ORCID = "0000-0002-7961-6646"))
